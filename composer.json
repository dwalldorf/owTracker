{
    "name": "owTracker",
    "license": "ISC",
    "type": "project",
    "description": "Allows you to track your CS:GO Overwatch cases and do useful stuff with that information",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "^3.1",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "symfony/console": "^3.1",
        "doctrine/mongodb-odm-bundle": "dev-master",
        "leaseweb/memcache-bundle": "^2.1",
        "php-amqplib/rabbitmq-bundle": "^1.10",
        "raveren/kint": "^1.0",
        "alcaeus/mongo-php-adapter": "^1.0",
        "netresearch/jsonmapper": "^0.11.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.1",
        "phpunit/phpunit": "^4.8"
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
