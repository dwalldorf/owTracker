{
    "name": "ow_tracker",
    "version": "0.0.6-SNAPSHOT",
    "description": "CS:GO Overwatch tracker",
    "keywords": [
        "Counter-Strike Global Offensive",
        "Counter Strike",
        "Global Offensive",
        "CS:GO",
        "CSGO",
        "Overwatch"
    ],
    "main": "app.php",
    "dependencies": {
        "@angular/common": "2.0.0-rc.1",
        "@angular/compiler": "2.0.0-rc.1",
        "@angular/core": "2.0.0-rc.1",
        "@angular/http": "2.0.0-rc.1",
        "@angular/platform-browser": "2.0.0-rc.1",
        "@angular/platform-browser-dynamic": "2.0.0-rc.1",
        "@angular/router": "2.0.0-rc.1",
        "@angular/router-deprecated": "2.0.0-rc.1",
        "@angular/upgrade": "2.0.0-rc.1",
        "angular2-in-memory-web-api": "0.0.7",
        "bootstrap": "^3.3.6",
        "es6-shim": "^0.35.0",
        "jquery": "^2.2.2",
        "ng2-file-upload": "^1.0.3",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.6",
        "spin.js": "^2.3.2",
        "systemjs": "0.19.27",
        "zone.js": "^0.6.12"
    },
    "devDependencies": {
        "concurrently": "^2.0.0",
        "lite-server": "^2.2.0",
        "ntypescript": "^1.201605062206.1",
        "typings": "^0.8.1"
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build": "ntsc -p ./web/app",
        "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
        "lite": "lite-server",
        "postinstall": "typings install",
        "tsc": "tsc --outDir web/app/dist",
        "tsc:w": "tsc --outDir web/app/dist --w",
        "typings": "typings",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dwalldorf/owTracker.git"
    },
    "author": "Daniel Walldorf",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dwalldorf/owTracker/issues"
    },
    "homepage": "https://github.com/dwalldorf/owTracker#readme"
}
